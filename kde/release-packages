#!/bin/bash

. common

for i in ${depPkgName} ${optPkgName}; do
	_pushd build/${i}/$svnbranch
	[[ -f ${i}-${pkgver}-${pkgrel}-any${PKGEXT} ]] && archrelease ${repo}-any
	[[ -f ${i}-${pkgver}-${pkgrel}-i686${PKGEXT} ]] && archrelease ${repo}-i686
	[[ -f ${i}-${pkgver}-${pkgrel}-x86_64${PKGEXT} ]] && archrelease ${repo}-x86_64
	_popd
done
