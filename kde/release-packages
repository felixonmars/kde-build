#!/bin/bash

. common

for i in ${depPkgName} ${optPkgName} kde-l10n; do
	_pushd build/${i}/$svnbranch
	echo -n "$i: "
	if [ -f oxygen-icons-${kdever}-*-any${PKGEXT} ]; then
		archrelease ${repo}-any
		continue
	elif [ -f kde-wallpapers-${kdever}-*-any${PKGEXT} ]; then
		archrelease ${repo}-any
		continue
	elif [ "$i" == "kde-l10n" -a ${l10n} ]; then
		archrelease ${repo}-any
		continue
	fi

	for a in i686 x86_64; do
		if [ -f ${i}-*-${kdever}-*-${a}${PKGEXT} ]; then
			archrelease ${repo}-${a}
		else
			echo "skipping ${a}"
		fi
	done
	_popd
done