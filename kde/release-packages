#!/bin/bash

. common

for i in ${depPkgName} ${optPkgName} kde-l10n; do
	_pushd build/${i}/$svnbranch
	echo -n "$i: "
	if [ -f "oxygen-icons-${pkgver}-${pkgrel}-any${PKGEXT}" ]; then
		archrelease ${repo}-any
		continue
	elif [ "$i" = "kde-l10n" -a ${l10n} ]; then
		archrelease ${repo}-any
		continue
	fi

	for a in i686 x86_64; do
		if [ -f "${i}-${pkgver}-${pkgrel}-${a}${PKGEXT}" ]; then
			archrelease ${repo}-${a}
		else
			echo "skipping ${a}"
		fi
	done
	_popd
done