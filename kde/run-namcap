#!/bin/bash

. common

for i in ${depPkgName} ${optPkgName}; do
	_pushd build/${i}/${svnbranch}
	unset pkgname
    unset pkgver
    unset pkgrel
	. PKGBUILD
	for j in ${pkgname[@]}; do
		if [ -f ${j}-${pkgver}-${pkgrel}-${CARCH}${PKGEXT} ]; then
			pkgfile=${j}-${pkgver}-${pkgrel}-${CARCH}${PKGEXT}
			echo "checking ${pkgfile}..."
			namcap ${pkgfile} > ${pkgfile}-namcap.log
		fi
	done
	_popd
done
